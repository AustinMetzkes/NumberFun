@{
    ViewData["Title"] = "Home Page";
}
<h2>Enter a Number</h2>

<div class="row">
    <form class="col s12">
        <div class="row">
            <div class="input-field col s6">
                <input id="Number" type="text" class="validate">
                <label for="Number">Enter In Your Number</label>
            </div>
                <div class="my button">
                    <button class="btn waves-effect waves-light  green darken-1" type="submit" name="action">
                        Analyze
                        <i class="material-icons right">send</i>
                    </button>
                </div>
            </div>
    </form>
</div>
<div class="my scroll">
    <div class="row">
        <div class="col s12 m9 l10">
        </div>
        <div class="col hide-on-small-only m3 l2">
            <ul class="section table-of-contents">
                <li><a href="#Prime">Prime</a></li>
                <li><a href="#Perfect">Perfect</a></li>
                <li><a href="#Squareful">Sqaureful</a></li>
                <li><a href="#Squarefull">Squarefull</a></li>
                <li><a href="#Palindromic">Palindromic</a></li>
            </ul>
        </div>
    </div>
    </div>






